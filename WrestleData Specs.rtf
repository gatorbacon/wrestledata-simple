\f8\i \cf2 import time\
import random\
\pard\pardeftab720\sl48\slmult1\sa240\partightenfactor0

\f3\i0 \cf2 \
\pard\pardeftab720\sl48\slmult1\sa240\partightenfactor0

\f8\i \cf2 # Between 1.5 and 3 seconds\
